<div role="document" class="page">
  {%  if page.header %}
    <header id="site-header">
      <div class="outer-container">
        {{ page.header }}
      </div>
    </header>
  {% endif %}

  {%  if page.hero %}
    <section id="hero-header">
      <div class="hero-container">
        {{ page.hero }}
      </div>
    </section>
  {% endif %}

  {%  if page.featured %}
    <section id="featured">
      <div class="columns">
        {{ page.featured }}
      </div>
    </section>
  {% endif %}

  {% if page.breadcrumb %}
    <section id="breadcrumb">
      <div class="outer-container">
        {{ page.breadcrumb }}
      </div>
    </section>
  {% endif %}

  <div class="main-wrapper">
  <main role="main" class="outer-container">
    <a id="main-content" tabindex="-1"></a>

    <aside id="az-sidebar-first" role="complementary" class="az-sidebar">
      {{ page.sidebar_first }}
    </aside>

    <section id="content">
      {{ page.content }}
    </section>

    <aside id="az-sidebar-second" role="complementary" class="az-sidebar">
      {{ page.sidebar_second }}
    </aside>
  </main>
  </div>

  {% include('@sa_neato/include/footer.html.twig') %}
</div>
